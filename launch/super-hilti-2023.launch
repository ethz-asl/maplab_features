<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="config_file" default="$(find maplab_features)/config/super-hilti2023.yaml"/>
  <node name="super" pkg="maplab_features" type="ros_interface.py" output="screen">
    <rosparam command="load" file="$(arg config_file)"/>
    <param name="pca_pickle_path" value="$(find maplab_features)/share/superpoint_pca_hilti2023.txt"/>
  </node>
</launch>
