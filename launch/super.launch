<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="config_file" default="$(find maplab_features)/config/super.yaml"/>
  <arg name="node_name"   default="super"/>
  <node name="$(arg node_name)" pkg="maplab_features" type="ros_interface.py" output="screen">
    <rosparam command="load" file="$(arg config_file)"/>
    <param name="path_prefix" value="$(find maplab_features)"/>
  </node>
</launch>
